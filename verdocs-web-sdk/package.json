    "air-datepicker": "^3.6.0",
    "choices.js": "^11.1.0",
    "date-fns": "^4.1.0",
    "imask": "^7.6.1",
    "interactjs": "^1.10.27",
    "jszip": "^3.10.1",
    "sortablejs": "^1.15.6",
    "tinybase": "^6.5.1",
    "workflow-svg.js": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@interactjs/types": "^1.10.27",
