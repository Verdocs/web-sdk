{
  "name": "@verdocs/web-sdk",
  "version": "1.0.25",
  "private": false,
  "description": "Verdocs Web SDK",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/verdocs-web-sdk/verdocs-web-sdk.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "serve": "stencil build --dev --watch --serve --es5",
    "start": "stencil build --dev --watch --docs --es5",
    "test": "stencil test --spec --e2e --screenshot",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "BROWSER=none start-storybook -p 6006 -s public",
    "build-storybook": "rm -rf storybook-static && build-storybook -s public",
    "copy-storybook": "cp -aR storybook-static ../../partner-portal/site/static/embeds",
    "clear-docs": "aws --profile=verdocs cloudfront create-invalidation --distribution-id E2V8LVV0H1KJQ7 --paths \"/*\"",
    "deploy-docs": "npm run build-storybook && aws --profile=verdocs s3 sync --acl public-read --delete storybook-static s3://verdocs-developers-embeds/ && yarn clear-docs"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "@stencil/core": "^2.10.0",
    "@verdocs/js-sdk": "^1.1.15",
    "pdfjs-dist": "2.12.313",
    "vanillajs-datepicker": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@pxtrn/storybook-addon-docs-stencil": "^6.4.0",
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/react-output-target": "^0.2.1",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-actions": "6.4.13",
    "@storybook/addon-docs": "6.4.13",
    "@storybook/addon-essentials": "6.4.13",
    "@storybook/addon-links": "6.4.13",
    "@storybook/builder-webpack5": "^6.4.13",
    "@storybook/manager-webpack5": "^6.4.13",
    "@storybook/web-components": "6.4.13",
    "@types/css-font-loading-module": "^0.0.7",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.6",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "lit-html": "^2.0.2",
    "prettier": "^2.5.1",
    "puppeteer": "^10.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "stencil-inline-svg": "^1.1.0",
    "terser-webpack-plugin": "^5.3.0",
    "typescript": "^4.5.4",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
