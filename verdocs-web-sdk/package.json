{
  "name": "@verdocs/web-sdk",
  "version": "6.0.5",
  "private": false,
  "description": "Verdocs Web SDK",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "web-types": "dist/types/web-types.json",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "unpkg": "dist/verdocs-web-sdk/verdocs-web-sdk.esm.js",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "push": "npm run build && npm publish",
    "build": "stencil build --docs",
    "serve": "stencil build --dev --watch --serve",
    "start": "stencil build --dev --watch --docs",
    "test": "stencil test --spec",
    "test-coverage": "stencil test --spec --coverage",
    "test-visualdiff": "stencil test -- src/components/controls/ --e2e --screenshot",
    "e2e-test": "stencil test --e2e",
    "test.watch": "stencil test --spec --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@stencil/core": "^4.36.1",
    "@svgdotjs/svg.js": "^3.2.4",
