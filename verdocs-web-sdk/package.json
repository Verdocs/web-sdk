{
  "name": "@verdocs/web-sdk",
  "version": "1.7.9",
  "private": false,
  "description": "Verdocs Web SDK",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "unpkg": "dist/verdocs-web-sdk/verdocs-web-sdk.esm.js",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "fixup-pdfjs.js",
    "dist/",
    "loader/"
  ],
  "scripts": {
    "postinstall": "node fixup-pdfjs.js",
    "build": "stencil build --docs",
    "buildX": "stencil build --docs && cp www/build/verdocs-web-sdk.css ../verdocs-web-sdk-react/src",
    "serve": "stencil build --dev --watch --serve --es5",
    "start": "stencil build --dev --watch --docs --es5",
    "test": "stencil test --spec --e2e --screenshot",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "NODE_DEBUG=true rm -rf storybook-static && BROWSER=none start-storybook -p 6006 --no-open",
    "build-storybook": "NODE_DEBUG=true rm -rf storybook-static && build-storybook"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "@stencil/core": "^2.17.4",
    "@svgdotjs/svg.js": "^3.1.2",
    "@verdocs/js-sdk": "^2.0.15",
    "pdfjs-dist": "^2.16.105",
    "vanillajs-datepicker": "^1.2.0",
    "workflow-svg.js": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@pxtrn/storybook-addon-docs-stencil": "^6.4.1",
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/react-output-target": "^0.3.1",
    "@stencil/sass": "^1.5.2",
    "@stencil/store": "^2.0.0",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-docs": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/builder-webpack5": "6.5.10",
    "@storybook/manager-webpack5": "6.5.10",
    "@storybook/theming": "^6.5.10",
    "@storybook/web-components": "6.5.10",
    "@types/css-font-loading-module": "^0.0.7",
    "@types/jest": "^28.1.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/react": "^18.0.18",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.2",
    "jest-cli": "^28.1.2",
    "jest-svg-transformer": "^1.0.0",
    "lit-html": "^2.3.1",
    "prettier": "^2.7.1",
    "puppeteer": "^16.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-typescript": "^1.0.1",
    "stencil-inline-svg": "^1.1.0",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.7.4",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
