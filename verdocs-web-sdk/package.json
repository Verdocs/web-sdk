    "@stencil/sass": "^3.2.2",
    "@stencil/vue-output-target": "^0.10.8",
    "@types/jest": "^29.5.14",
    "babel-loader": "^10.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-svg-transformer": "^1.0.0",
    "prettier": "^3.6.2",
    "puppeteer": "^24.15.0",
    "terser-webpack-plugin": "^5.3.11",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
