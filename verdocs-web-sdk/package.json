{
  "name": "@verdocs/web-sdk",
  "version": "1.3.0",
  "private": false,
  "description": "Verdocs Web SDK",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/verdocs-web-sdk/verdocs-web-sdk.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs && cp www/build/verdocs-web-sdk.css ../verdocs-web-sdk-react/src",
    "serve": "stencil build --dev --watch --serve --es5",
    "start": "stencil build --dev --watch --docs --es5",
    "test": "stencil test --spec --e2e --screenshot",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "BROWSER=none start-storybook -p 6006 -s public",
    "build-storybook": "rm -rf storybook-static && build-storybook -s public"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "@stencil/core": "^2.16.1",
    "@verdocs/js-sdk": "^1.3.0",
    "pdfjs-dist": "2.12.313",
    "vanillajs-datepicker": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@pxtrn/storybook-addon-docs-stencil": "^6.4.1",
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/react-output-target": "^0.3.1",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-docs": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/web-components": "6.5.9",
    "@types/css-font-loading-module": "^0.0.7",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.6",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "lit-html": "^2.2.6",
    "prettier": "^2.7.0",
    "puppeteer": "^14.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "stencil-inline-svg": "^1.1.0",
    "terser-webpack-plugin": "^5.3.3",
    "typescript": "^4.7.3",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
