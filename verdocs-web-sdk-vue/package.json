{
  "name": "@verdocs/web-sdk-vue",
  "version": "5.0.22",
  "description": "Verdocs Web SDK for React",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "push": "npm i && npm run build && npm publish",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p .",
    "rollup": "rollup -c"
  },
  "dependencies": {
    "@verdocs/web-sdk": "^5.0.22",
    "@stencil/vue-output-target": "^0.9.2"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "22.9.0",
    "jest": "^29.7.0",
    "jest-dom": "^4.0.0",
    "prettier": "^3.3.3",
    "vue": "^3.5.13",
    "typescript": "^5.6.3"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
