    "rollup": "rollup -c"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.63",
    "@types/react-dom": "^18.2.20",
    "jest": "^29.7.0",
    "jest-dom": "^4.0.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@verdocs/web-sdk": "^2.3.84"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
