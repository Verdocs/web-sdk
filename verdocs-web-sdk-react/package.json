{
  "name": "@verdocs/web-sdk-react",
  "version": "2.3.84",
  "description": "Verdocs Web SDK for React",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
