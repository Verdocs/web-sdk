{
  "name": "@verdocs/web-sdk-react",
  "version": "6.2.0-beta.4",
  "description": "Verdocs Web SDK for React",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "push": "npm i && npm run build && npm publish",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p .",
    "rollup": "rollup -c"
  },
  "dependencies": {
    "@verdocs/web-sdk": "^6.2.0-beta.4",
    "@stencil/react-output-target": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "22.14.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "jest": "^29.7.0",
    "jest-dom": "^4.0.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
