{
  "name": "@verdocs/web-sdk-react",
  "version": "5.0.24",
  "description": "Verdocs Web SDK for React",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
